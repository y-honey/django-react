{
  "projectName": "{{ projectName }}",
  "defaultStage": "{{ defaultStage }}",
  "aws": {
    "profile": "{{ awsProfile }}",
    "region": "{{ awsRegion }}"
  },
  "hostedZone": {
    "id": "{{ hostedZoneId }}",
    "name": "{{ hostedZoneName }}"
  },
  "certificate": {{ certificate }},
  "domains": {
    "{{ defaultStage }}": {
      "adminPanel": "admin.{{ defaultStage }}.{{ appDomain }}",
      "api": "api.{{ defaultStage }}.{{ appDomain }}",
      "webApp": "app.{{ defaultStage }}.{{ appDomain }}",
      "www": "www.{{ defaultStage }}.{{ appDomain }}"
    }
  }
}