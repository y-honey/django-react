{
  "deployBranches": ["{{ deployBranch }}"],
  "hostedZone": {
    "id": "{{ hostedZoneId }}",
    "name": "{{ hostedZoneName }}"
  },
  "certificates": {
    "cloudfrontCertificateArn": "{{ cloudfrontCertificateArn }}",
    "loadBalancerCertificateArn": "{{ loadBalancerCertificateArn }}",
    "domain": "{{ domain }}"
  },
  "basicAuth": "admin:{{ basicAuthPass }}",
  "domains": {
    "adminPanel": "admin.{{ domain }}",
    "api": "api.{{ domain }}",
    "webApp": "app.{{ domain }}",
    "www": "www.{{ domain }}",
    "docs": "docs.{{ domain }}",
    "cdn": "cdn.{{ domain }}"
  }
}
