{
  "name": "docs",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/internal/docs/src",
  "projectType": "library",
  "targets": {
    "build": {
      "dependsOn": [],
      "options": {
        "cwd": "packages/internal/docs"
      },
      "command": "pnpm run build:app"
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/internal/docs/**/*.{js,jsx,ts,tsx}"]
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "packages/internal/docs",
        "color": true,
        "commands": ["nx run backend:build-docs", "pnpm run build"],
        "parallel": false
      }
    },
    "start": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "packages/internal/docs",
        "color": true,
        "commands": ["nx run backend:build-docs", "pnpm start"],
        "parallel": false
      }
    }
  },
  "tags": []
}
