{
  "name": "infra",
  "version": "0.0.0",
  "scripts": {
    "deploy:global": "cdk deploy *GlobalStack && cdk deploy *UsEastResourcesStack",
    "deploy:global-tools": "env-cmd -f ../../../.env.tools cdk deploy *GlobalToolsStack",
    "deploy:main": "cdk deploy *MainStack",
    "deploy:db": "cdk deploy *DbStack",
    "deploy:ci": "cdk deploy *CiStack",
    "deploy:components": "cdk deploy *ComponentsStack",
    "deploy:api": "cdk deploy *ApiStack",
    "deploy:migrations": "cdk deploy *MigrationsStack",
    "deploy:webapp": "cdk deploy *WebAppStack",
    "deploy:docs": "cdk deploy *DocsStack",
    "deploy:env": "nx run infra:deploy:main && nx run infra:deploy-db && nx run infra:deploy-db"
  },
  "devDependencies": {
    "@types/node": "^10.17.28",
    "aws-cdk": "2.33.0",
    "ts-node": "^9.1.1",
    "env-cmd": "^10.1.0"
  },
  "dependencies": {
    "@aws-cdk/aws-apigatewayv2-alpha": "2.33.0-alpha.0",
    "@types/humps": "^1.1.3",
    "aws-cdk-lib": "2.33.0",
    "cdk-ec2-key-pair": "^3.3.1",
    "constructs": "^10.1.225",
    "humps": "^2.0.1"
  },
  "nx": {}
}
