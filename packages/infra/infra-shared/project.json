{
  "name": "infra-shared",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/infra/infra-shared/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/infra/infra-shared",
        "main": "packages/infra/infra-shared/src/index.ts",
        "tsConfig": "packages/infra/infra-shared/tsconfig.lib.json",
        "assets": ["packages/infra/infra-shared/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/infra/infra-shared/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "packages/infra/infra-shared/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
